# Vanta Speech Android - Configuration Template
# Copy this file to local.properties and fill in your values
# DO NOT commit local.properties to version control!

# API Configuration
# Base URL for the transcription API server
API_BASE_URL=http://your-server:8000/v1/

# API Key for authentication with the transcription server
API_KEY=your-api-key-here

# Azure AD Configuration (for Outlook Calendar integration)
# Register an app at https://portal.azure.com/#blade/Microsoft_AAD_RegisteredApps/ApplicationsListBlade
# Required permissions: Calendars.ReadWrite, User.Read, offline_access
AZURE_CLIENT_ID=your-azure-client-id-here

# Android SDK Configuration (auto-generated by Android Studio)
# sdk.dir=/path/to/android/sdk
